
def szukanie(tablica, liczba):
    for i in tablica:
        if i == liczba:
            return True
    return False

tablica = [9, 6, 8, 4, 71]
liczba = int(input("Wprowadz liczbÄ™: "))

if szukanie(tablica, liczba):
    print("Twoja liczba jest w tablicy!")
else:
    print("Twojej liczby nie ma w tablicy!")

